package com.dqgb.common.vo.bpmBusiness;

import java.util.Date;
import java.util.List;
import java.util.Map;

/**
 * ClassName:BpmInstanceVo <br/>
 * Function: 流程实例展示. <br/>
 * Date: 2018年5月22日 下午3:32:47 <br/>
 * 
 * @author wenzhang
 * @version
 * @since JDK 1.8
 * @see
 */
public class BpmInstanceVo {

	private String instanceId;// 流程实例表主键

	private String instanceTitle;// 流程实例标题

	private String instanceName;// 流程实例名称

	private String instanceKey;// 流程实例业务主键

	private Integer instanceStatus;// 流程实例状态 0 审批中 1 拒绝，2 同意，3 撤销，4驳回，5 挂起 6异常结束

	private Date startTime;// 流程实例发起时间

	private Date endTime;// 流程实例结束时间

	private String duration;// 流程实例耗时

	private String procinstId;// 流程实例id(引擎中的)

	private String instanceSponsor;// 流程实例发起者账号

	private String instanceSponsorName;// 流程实例发起者中文名

	private String categoryCode;// 流程实例分类编码

	private String modelKey;// 流程实例模板编码

	private List<BpmInstanceTaskVo> tasks;// 流程实例任务节点信息

	private Map<String, Object> instanceVariable;// 流程实例变量，含值

	private boolean isDone;// 流程实例是否结束

	public String getInstanceId() {
		return instanceId;
	}

	public void setInstanceId(String instanceId) {
		this.instanceId = instanceId;
	}

	public String getInstanceTitle() {
		return instanceTitle;
	}

	public void setInstanceTitle(String instanceTitle) {
		this.instanceTitle = instanceTitle;
	}

	public String getInstanceName() {
		return instanceName;
	}

	public void setInstanceName(String instanceName) {
		this.instanceName = instanceName;
	}

	public String getInstanceKey() {
		return instanceKey;
	}

	public void setInstanceKey(String instanceKey) {
		this.instanceKey = instanceKey;
	}

	public Integer getInstanceStatus() {
		return instanceStatus;
	}

	public void setInstanceStatus(Integer instanceStatus) {
		this.instanceStatus = instanceStatus;
	}

	public Date getStartTime() {
		return startTime;
	}

	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}

	public Date getEndTime() {
		return endTime;
	}

	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}

	public String getDuration() {
		return duration;
	}

	public void setDuration(String duration) {
		this.duration = duration;
	}

	public String getProcinstId() {
		return procinstId;
	}

	public void setProcinstId(String procinstId) {
		this.procinstId = procinstId;
	}

	public String getInstanceSponsor() {
		return instanceSponsor;
	}

	public void setInstanceSponsor(String instanceSponsor) {
		this.instanceSponsor = instanceSponsor;
	}

	public String getInstanceSponsorName() {
		return instanceSponsorName;
	}

	public void setInstanceSponsorName(String instanceSponsorName) {
		this.instanceSponsorName = instanceSponsorName;
	}

	public String getCategoryCode() {
		return categoryCode;
	}

	public void setCategoryCode(String categoryCode) {
		this.categoryCode = categoryCode;
	}

	public String getModelKey() {
		return modelKey;
	}

	public void setModelKey(String modelKey) {
		this.modelKey = modelKey;
	}

	public List<BpmInstanceTaskVo> getTasks() {
		return tasks;
	}

	public void setTasks(List<BpmInstanceTaskVo> tasks) {
		this.tasks = tasks;
	}

	public Map<String, Object> getInstanceVariable() {
		return instanceVariable;
	}

	public void setInstanceVariable(Map<String, Object> instanceVariable) {
		this.instanceVariable = instanceVariable;
	}

	public boolean isDone() {
		return isDone;
	}

	public void setDone(boolean isDone) {
		this.isDone = isDone;
	}

}
