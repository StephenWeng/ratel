package com.dqgb.common.vo.bpmBusiness;

/**
 * ClassName:BpmInstanceTaskVo <br/>
 * Function: 流程实例任务节点vo. <br/>
 * Date: 2018年5月22日 下午3:41:05 <br/>
 * 
 * @author wenzhang
 * @version
 * @since JDK 1.8
 * @see
 */
public class BpmInstanceTaskVo {

	private String taskNodeId;// 流程节点id

	private String taskName;// 流程节点名称

	private Integer isCountersign;// 是否会签任务：0否，1是

	private String taskId;// 任务id

	private Integer taskStatus;// 审批状态：-1待审批，0 审批中， 1 拒绝，2 同意，3 撤销，4驳回，5 挂起 6异常

	private String taskApproveUser;// 审批人

	private String taskCandidateUser;// 待审批人

	private String taskOpinion;// 审批意见

	private String taskEndTime;// 结束时间

	private String taskStartTime;// 开始时间

	public String getTaskNodeId() {
		return taskNodeId;
	}

	public void setTaskNodeId(String taskNodeId) {
		this.taskNodeId = taskNodeId;
	}

	public String getTaskName() {
		return taskName;
	}

	public void setTaskName(String taskName) {
		this.taskName = taskName;
	}

	public Integer getIsCountersign() {
		return isCountersign;
	}

	public void setIsCountersign(Integer isCountersign) {
		this.isCountersign = isCountersign;
	}

	public String getTaskId() {
		return taskId;
	}

	public void setTaskId(String taskId) {
		this.taskId = taskId;
	}

	public Integer getTaskStatus() {
		return taskStatus;
	}

	public void setTaskStatus(Integer taskStatus) {
		this.taskStatus = taskStatus;
	}

	public String getTaskApproveUser() {
		return taskApproveUser;
	}

	public void setTaskApproveUser(String taskApproveUser) {
		this.taskApproveUser = taskApproveUser;
	}

	public String getTaskCandidateUser() {
		return taskCandidateUser;
	}

	public void setTaskCandidateUser(String taskCandidateUser) {
		this.taskCandidateUser = taskCandidateUser;
	}

	public String getTaskOpinion() {
		return taskOpinion;
	}

	public void setTaskOpinion(String taskOpinion) {
		this.taskOpinion = taskOpinion;
	}

	public String getTaskEndTime() {
		return taskEndTime;
	}

	public void setTaskEndTime(String taskEndTime) {
		this.taskEndTime = taskEndTime;
	}

	public String getTaskStartTime() {
		return taskStartTime;
	}

	public void setTaskStartTime(String taskStartTime) {
		this.taskStartTime = taskStartTime;
	}

}
